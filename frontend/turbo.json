{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".nuxt/**", ".output/**"]
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": [".output/**"]
    },
    "preview": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    }
  }
}
